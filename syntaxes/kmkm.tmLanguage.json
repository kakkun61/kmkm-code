{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Kmkm",
  "patterns": [
    {
      "name": "keyword.other.kmkm",
      "match": "\\b(module|define|list|bind|function|function1|procedure|apply|var)\\b"
    },
    {
      "name": "variable.other.kmkm",
      "match": "('module'|'define'|'list'|'bind'|'function'|'function1'|'procedure'|'apply'|'var')"
    },
    {
      "name": "string.quoted.double.kmkm",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.kmkm",
          "match": "\\\\."
        }
      ]
    },
    {
      "name": "string.quoted.single.kmkm",
      "match": "'(\\\\'|\\\\\\\\|[^\\\\])'"
    }
  ],
  "scopeName": "source.kmkm"
}
